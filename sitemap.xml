---
rooturl: http://timbul.github.com
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
   <url>
      <loc>http://timbul.github.com/</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>1</priority>
   </url>
   <url>
      <loc>http://timbul.github.com/archives</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.9</priority>
   </url>
   <url>
      <loc>http://timbul.github.com/about</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.7</priority>
   </url>
   <url>
      <loc>http://timbul.github.com/atom.xml</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>daily</changefreq>
      <priority>0.5</priority>
   </url>
{% for post in site.posts %}   <url>
      <loc>{{ page.rooturl }}{{ post.url }}</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.8</priority>
   </url>
{% endfor %}</urlset>